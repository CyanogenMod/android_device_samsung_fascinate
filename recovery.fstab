# mount point	fstype    device    	[device2]    [fsoptions]     [fstype2]     [fsoptions2]

/boot       bml     /dev/block/bml7
/cache      rfs     /dev/block/stl11       llw,check=no,nosuid,nodev
/data       rfs     /dev/block/mmcblk0p1   llw,check=no,nosuid,nodev
/datadata   rfs     /dev/block/stl10       llw,check=no,nosuid,nodev
/recovery   bml     /dev/block/bml8
/sdcard     vfat    /dev/block/mmcblk1p1
/system     rfs     /dev/block/stl9        llw,check=no
