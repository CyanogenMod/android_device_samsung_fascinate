# mount     fstype  device                  [device2]   [fsoptions]    [fstype2]     [fsoptions2]

/boot       bml     /dev/block/bml7
/cache      ext4    /dev/block/stl11        NULL        NULL            rfs         llw,check=no,nosuid,nodev
/data       ext4    /dev/block/mmcblk0p1    NULL        NULL            rfs         llw,check=no,nosuid,nodev
/datadata   ext4    /dev/block/stl10        NULL        NULL            rfs         llw,check=no,nosuid,nodev
/recovery   bml     /dev/block/bml8
/sdcard     vfat    /dev/block/mmcblk1p1
/system     ext4    /dev/block/stl9         NULL        NULL            rfs         llw,check=no
